{
    "version" : 2,
    "builds" : [
        {
            "src" : "index.js",
            "use" : "@vercel/node"
        }
    ],
    "functions": {
    "api/v1/user/login": {
      "maxDuration": 30
    }
  },
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "index.js"
        }
    ]
}